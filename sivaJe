pipeline {
   tools {
      jdk 'java'
	  maven 'Maven'
	  }
	  agent any
	  stages {
	   stage ('checkout') {
	     steps {
		    git 'https://github.com/sivareddy4444/Devopsclasscodes.git'
		}
	}
	   stage ('compile') {
	     steps {
		    sh 'mvn compile'
		}
	}
     stage ('codereview') {
        steps {
          sh 'mvn pmd:pmd'
         }
      }
      stage ('unit test') {
        steps {
         sh 'mvn test'
          }
     }
     stage ('package') {
        steps {
         sh 'mvn package'
        }
      }
   }
}
   
